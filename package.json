{
  "name": "magnet-router",
  "version": "3.2.0",
  "description": "Readme",
  "keywords": [
    "magnet",
    "magnetjs",
    "router"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-router#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-router/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-router",
  "scripts": {
    "build": "tsc",
    "dist": "npm run build && npm publish",
    "start": "tsc",
    "test": "babel-node tests/index.js | tap-spec",
    "watch": "nodemon --ignore dist --exec \"tsc --source-maps\"",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec babel-node tests/index.js | tap-spec"
  },
  "dependencies": {
    "boom": "^4.2.0",
    "koa-router": "^7.0.1",
    "magnet-core": "^3.5.3",
    "magnet-koa": "^1.2.1"
  },
  "devDependencies": {
    "concurrently": "^3.3.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
