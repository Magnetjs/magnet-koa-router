{
  "name": "magnet-router",
  "version": "3.1.0",
  "description": "Readme",
  "keywords": [
    "magnet",
    "magnetjs",
    "router"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-router#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-router/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-router",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dist": "npm run build && npm publish",
    "start": "babel src --out-dir dist",
    "test": "babel-node tests/index.js | tap-spec",
    "watch": "nodemon --ignore dist --exec \"babel src --out-dir dist --source-maps\"",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec babel-node tests/index.js | tap-spec"
  },
  "babel": {
    "ignore": [
      "docs",
      "node_modules",
      "dist",
      "test"
    ],
    "plugins": [
      "fast-async"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "boom": "^4.2.0",
    "koa-router": "^7.0.1",
    "magnet-core": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "concurrently": "^3.3.0",
    "fast-async": "^6.2.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
